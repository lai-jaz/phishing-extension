{
  "manifest_version": 3,
  "name": "Phishing Detector Extension",
  "version": "1.0",
  "description": "Phishing detection.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/iconDanger.png", "images/iconSus.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "storage", 
    "activeTab", 
    "scripting",
    "tabs",
    "webRequest",
    "notifications"],
  "host_permissions":["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }

}
